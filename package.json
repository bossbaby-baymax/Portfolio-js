{
  "name": "portfolio-js",
  "private": true,
  "description": "A unique collection of developer portfolio templates",
  "keywords": [
    "portfolio",
    "astro",
    "porfolio-template",
    "astrojs",
    "astro-template"
  ],
  "bugs": {
    "url": "https://github.com/bossbaby-baymax/Portfolio-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bossbaby-baymax/Portfolio-js"
  },
  "license": "MIT",
  "author": "Full-Stack Developer - Jordan Wooding <jordan.wooding92@gmail.com>",
  "scripts": {
    "eslint": "eslint .",
    "dev": "pnpm -C unicorn-sparkle dev",
    "build": "pnpm -C unicorn-sparkle build output .unicorn-sparkle/dist",
    "prepare": "husky",
    "preview": "pnpm -C unicorn-sparkle preview"
  },
  "lint-staged": {
    "./**/*.{js,mjs,cjs}": [
      "eslint --cache --fix --ext .js,.mjs,.cjs ./",
      "prettier --cache --write \"./**/*.{js,mjs,cjs}\""
    ],
    "./**/*.ts": [
      "eslint --cache --fix --ext .ts ./",
      "prettier --cache --write \"./**/*.ts\""
    ],
    "./**/*.astro": [
      "eslint --cache --fix --ext .astro ./",
      "prettier --cache --write \"./**/*.astro\""
    ],
    "./**/*.css": "prettier --cache --write \"./**/*.css\"",
    "./**/*.json": "prettier --cache --write \"./**/*.json\"",
    "./**/*.yml": "prettier --cache --write \"./**/*.yml\""
  },
  "overrides": {
    "astro-tunnel": {
      "astro": "5.0.4"
    }
  },
  "dependencies": {
    "@astrolicious/i18n": "0.6.0",
    "@fontsource-variable/inter": "5.1.0",
    "@fontsource/inter": "5.1.0",
    "@jridgewell/gen-mapping": "0.3.5",
    "@playform/compress": "0.1.6",
    "astro": "5.0.4",
    "astro-compressor": "1.0.0",
    "astro-icon": "1.1.4",
    "astro-imagetools": "0.9.0",
    "astro-seo-schema": "5.0.0",
    "astro-sitemap": "1.0.0",
    "astro-tunnel": "0.1.6",
    "lighthouse-badges": "1.3.7",
    "sanitize.css": "13.0.0",
    "sharp": "0.33.5"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.4.0",
    "@typescript-eslint/eslint-plugin": "7.0.1",
    "@typescript-eslint/parser": "7.18.0",
    "astro-eslint-parser": "1.1.0",
    "eslint": "8.57.0",
    "eslint-config-love": "47.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-astro": "1.3.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-promise": "6.1.1",
    "husky": "9.1.7",
    "lint-staged": "^15.2.10",
    "prettier": "3.4.2",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-tailwindcss": "0.6.6"
  }
}
